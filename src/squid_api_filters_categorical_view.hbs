<div style="margin-top: 20px;" class="panel panel-default filter-panel squid_api_filters-categorical-panel-view">
	<div class="panel-heading">
		<button type="button" class="close" data-toggle="collapse"
			data-target="{{data-target}}" data-clavier="true" aria-hidden="true">
			<i class="glyphicon glyphicon-chevron-up"></i>
		</button>
		<h4 class="panel-title" id="myModalLabel">Filters</h4>
	</div>
	<div class="panel-body">
		<div class="row">
			<div class="col-md-4">
				<h4>Filter Store</h4>
			</div>
			<div class="col-md-4">
				<div id="filter-selection">
					<select type="button" class="btn btn-select-filter">Choose Filter<span class="caret"></span></select>
				</div>
			</div>
			<div class="col-md-2">
				<div id="search">
				<form action="#" role="search">
                <div class="form-group">
                  <div class="input-group">
                    <input class="form-control" id="navbarInput-01" type="search" placeholder="Search">
                    <span class="input-group-btn">
                      <button type="submit" class="btn"><i class="fa fa-search"></i></button>
                    </span>
                  </div>
                </div>
              </form>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-4">
				<div id="selected">
					
				</div>
			</div>
			<div class="col-md-8">
			<div id="filter-display-results">

			</div>
			</div>
		</div>
		<div class="row">
		<div class="col-md-4">
		</div>
		<div class="col-md-8">
				<div id="pagination-container">
					
				</div>
			</div>
	</div>
	<div class="panel-footer">
		<button type="button" class="btn btn-primary apply-selection" data-toggle="collapse"
			data-target="{{data-target}}" data-clavier="true">Apply</button>
		<button type="button" class="btn btn-default cancel-selection" data-toggle="collapse"
			data-target="{{data-target}}" data-clavier="true">Cancel</button>
	</div>
</div>